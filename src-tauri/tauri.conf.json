{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "MusicForMom",
  "version": "0.1.0",
  "identifier": "com.musicformom.app",
  "build": {
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Music For Mom",
        "width": 520,
        "height": 650,
        "resizable": true,
        "fullscreen": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
